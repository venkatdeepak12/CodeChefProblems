 /*
 
 Shahid and Sandeep were out fencing, when Sandeep finds extra swords in the fields.
The swords have been labelled with some numbers − denoting the size of each sword.
Your task is to find the A and B − where A is the sword with the shortest height and B with the longest height. Can you do this?

Sample Input:
1
4
3 4 1 2

Sample Output:
1 4

*/

//Code:

#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    long long t,n,i;
    cin>>t;
    while(t--)
    {
        cin>>n;
        long long a[n];
        for(i=0;i<n;i++)
        {
            cin>>a[i];
        }
        sort(a,a+n);
        cout<<a[0]<<" "<<a[n-1]<<endl;
    }
    return  0;
}
